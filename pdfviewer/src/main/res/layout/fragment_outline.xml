<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:id="@+id/id_outline_list"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:divider="@null"
        android:listSelector="@drawable/selector_listview_item"/>

    <RelativeLayout
        android:id="@+id/id_outline_miss"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible">

        <ImageView
            android:id="@+id/id_outline_list_iv"
            android:layout_width="@dimen/qb_px_100"
            android:layout_height="@dimen/qb_px_110"
            android:layout_centerInParent="true"
            android:src="@drawable/ic_outline_dis"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_outline_list_iv"
            android:layout_centerHorizontal="true"
            android:text="@string/out_line_diss"
            android:textSize="@dimen/qb_px_18"/>
    </RelativeLayout>

</FrameLayout>